{
	"license": "MIT",
	"main": "./main.js",
	"name": "ng-xterm",
	"private": true,
	"productName": "NG-Xterm",
	"repository": "https://github.com/payneusmc07/ng-xterm.git",
	"scripts": {
		"build:electron-only": "yarn electron:tsc && electron-builder build -p never",
		"build:electron-only:all": "yarn electron:tsc && electron-builder build -p never -mlw",
		"build:final:all-platforms:no-publish": "yarn build:prod &&  electron-builder build -mlw -p never",
		"build:final:all-platforms:publish": "yarn build:prod  && electron-builder build -mlw -p always",
		"build:final:no-publish": "yarn build:prod && electron-builder build --publish=never",
		"build:final:publish": "yarn build:prod  && electron-builder build --publish=always",
		"build:local": "ng build",
		"build:prod": "yarn electron:tsc && ng build --base-href ./",
		"clean": "node bin/rimraf-standalone.js node_modules",
		"compodoc": "compodoc -p tsconfig.doc.json -w -s",
		"dev": "concurrently  \"yarn electron:serve\" \"yarn ng:serve\" --kill-others",
		"electron:local": "yarn electron:tsc && electron .",
		"electron:serve": "wait-on http://localhost:4200 && yarn electron:local",
		"electron:tsc": "tsc -p tsconfig.main.json",
		"init": "yarn install",
		"ng": "ng",
		"ng:serve": "ng serve -c dev",
		"postinstall": "electron-builder install-app-deps"
	},
	"version": "1.0.0",
	"author": "pusmc07",
	"browserslist": [
		"chrome 91"
	],
	"keywords": [
		"electron",
		"terminal",
		"node-pty",
		"xterm",
		"xterm-256color",
		"angular",
		"pty",
		"terminal-emulator"
	],
	"dependencies": {
		"electron-serve": "^1.1.0",
		"electron-store": "8.0.0",
		"electron-updater": "^4.3.9",
		"execa": "^5.1.1",
		"fs-extra": "10.0.0",
		"node-pty": "^0.10.1"
	},
	"description": "none",
	"devDependencies": {
		"@angular-builders/custom-webpack": "12.0.1-beta.0",
		"@angular-devkit/build-angular": "^12.0.4",
		"@angular-devkit/schematics": "12.0.4",
		"@angular/cli": "12.0.4",
		"@angular/common": "^12.0.4",
		"@angular/compiler": "^12.0.4",
		"@angular/compiler-cli": "12.0.4",
		"@angular/core": "^12.0.4",
		"@angular/forms": "^12.0.4",
		"@angular/platform-browser": "^12.0.4",
		"@angular/platform-browser-dynamic": "^12.0.4",
		"@angular/router": "^12.0.4",
		"@compodoc/compodoc": "^1.1.11",
		"@ng-bootstrap/ng-bootstrap": "11.0.0-beta.1",
		"@types/bootstrap": "5.0.13",
		"@types/sortablejs": "^1.10.6",
		"@types/node": "15.0.3",
		"bootstrap": "^5.0.1",
		"bootstrap-icons": "^1.5.0",
		"concurrently": "6.2.0",
		"electron": "13.1.1",
		"electron-builder": "22.10.5",
		"ng-terminal": "3.1.0",
		"ngx-sortablejs": "^11.1.0",
		"rxjs": "6.6.7",
		"sortablejs": "^1.13.0",
		"ts-node": "9.1.1",
		"tslib": "^2.0.0",
		"typescript": "~4.2.4",
		"wait-on": "5.3.0",
		"xterm-addon-unicode11": "^0.2.0",
		"xterm-addon-web-links": "^0.4.0",
		"xterm-addon-webgl": "^0.11.1",
		"zone.js": "~0.11.4"
	}
}
